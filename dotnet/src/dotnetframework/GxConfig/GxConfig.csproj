<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net461</TargetFramework>
		<OutputType>WinExe</OutputType>
		<ApplicationIcon>..\..\resources\Default_App.ico</ApplicationIcon>
		<RootNamespace>GxConfig</RootNamespace>
		<AssemblyName>GxConfig</AssemblyName>
		<IncludeCommonAssembly>false</IncludeCommonAssembly>
		<AllowUnsafeBlocks>false</AllowUnsafeBlocks>
		<NoWarn>CA1501;CA1824</NoWarn>
		<PackageTags>Config</PackageTags>
		<PackageId>GeneXus.Config</PackageId>
	</PropertyGroup>
	<ItemGroup>
	  <Content Include="..\..\resources\Default_App.ico" Link="Default_App.ico" />
	</ItemGroup>
	
  <ItemGroup>
    <ProjectReference Include="..\..\dotnetcommon\GxEncrypt\GxEncrypt.csproj" />
    <Reference Include="System.Drawing">
      <Name>System.Drawing</Name>
    </Reference>
    <Reference Include="System.Windows.Forms">
      <Name>System.Windows.Forms</Name>
    </Reference>
    <Reference Include="System.Xml">
      <Name>System.XML</Name>
    </Reference>
    <ProjectReference Include="..\GxClasses\GxClasses.csproj" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="app.config" />
  </ItemGroup>
</Project>