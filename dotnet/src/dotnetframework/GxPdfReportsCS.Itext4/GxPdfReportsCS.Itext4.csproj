<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net462</TargetFramework>
		<RootNamespace>GxPdfReportsCS</RootNamespace>
		<AssemblyName>GxPdfReportsCS</AssemblyName>
		<RunCodeAnalysis>false</RunCodeAnalysis>
		<PackageTags>Itext PDF Report using itext4x</PackageTags>
		<PackageId>GeneXus.PdfReportsCS.itext4</PackageId>
		<DefineConstants>ITEXT4</DefineConstants>
	</PropertyGroup>
	<ItemGroup>
	  <None Remove="pdfreportitext4.csback" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Include="..\GxPdfReportsCS\GlobalSuppressions.cs" Link="GlobalSuppressions.cs" />
	  <Compile Include="..\GxPdfReportsCS\PdfReportInterface.cs" Link="PdfReportInterface.cs" />
	  <Compile Include="..\GxPdfReportsCS\PDFReportItext.cs" Link="PDFReportItext.cs" />
	  <Compile Include="..\GxPdfReportsCS\Properties\AssemblyInfo.cs" Link="Properties\AssemblyInfo.cs" />
	</ItemGroup>


	<ItemGroup>
		<PackageReference Include="log4net" Version="2.0.11" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\GxClasses\GxClasses.csproj" />
	</ItemGroup>
	<ItemGroup>
	  <Reference Include="itextsharp">
	    <HintPath>..\libs\itextsharp4\itextsharp.dll</HintPath>
	  </Reference>
	</ItemGroup>
	<ItemGroup>
	  <Folder Include="Properties\" />
	</ItemGroup>

</Project>