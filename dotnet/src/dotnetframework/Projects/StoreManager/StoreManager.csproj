<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net462</TargetFramework>
		<RootNamespace>GeneXus.SD.Store</RootNamespace>
		<AssemblyName>GeneXus.SD.Store.StoreManager</AssemblyName>
		<SignAssembly>true</SignAssembly>
		<DefineConstants>TRACE;DEBUG;PACKAGE_INDEPENDENT_VERSION;PACKAGE_INDEPENDENT_FILEVERSION;PACKAGE_COPYRIGHT_START_YEAR</DefineConstants>
		<PackageTags>StoreManager</PackageTags>
		<PackageId>GeneXus.StoreManager</PackageId>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Google.Apis.AndroidPublisher.v3" Version="1.55.0.2582" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
		<PackageReference Include="Zlib.Portable.Signed" Version="1.11.0" />
	</ItemGroup>
	<ItemGroup>
		<ProjectReference Include="..\..\GxClasses\GxClasses.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="Jayrock-JSON">
			<SpecificVersion>False</SpecificVersion>
			<HintPath>..\..\libs\Jayrock-JSON.dll</HintPath>
		</Reference>

	</ItemGroup>
</Project>