<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net471</TargetFramework>
		<IsPublishable>false</IsPublishable>
		<RootNamespace>GamTest</RootNamespace>
		<AssemblyName>GamTest</AssemblyName>
		<NoWarn>CS0618,CA1707</NoWarn>
		<IsPackable>false</IsPackable>
	</PropertyGroup>
	
	 <ItemGroup>
		<PackageReference Include="Microsoft.CodeCoverage" Version="16.10.0" />
		<PackageReference Include="Microsoft.NET.Test.Sdk" Version="16.10.0" />
		<PackageReference Include="NUnit" Version="3.13.2" />
		<PackageReference Include="NUnit3TestAdapter" Version="4.6.0" />
		<PackageReference Include="jose-jwt" Version="5.0.0" />
		<PackageReference Include="StrongNamer" Version="0.2.5" />
	</ItemGroup>
	
	 <ItemGroup>
	   <Folder Include="Resources\" />
	 </ItemGroup>
	
	 <ItemGroup>
	   <ProjectReference Include="..\..\..\src\DotNetFramework\GamSaml20\GamSaml20.csproj" />
	 </ItemGroup>

	<ItemGroup>
		<None Update="Resources\dummycerts\RSA_sha256_2048\sha256_cert.cer">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="Resources\dummycerts\RSA_sha256_2048\sha256_cert.crt">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
		<None Update="Resources\dummycerts\RSA_sha256_2048\sha256_cert.key">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
